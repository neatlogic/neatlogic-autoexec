<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="codedriver.module.autoexec.dao.mapper.AutoexecScriptValidateMapper">

    <select id="getCodeListByType" parameterType="java.lang.String" resultType="codedriver.framework.autoexec.dto.script.AutoexecScriptValidateVo">
        select
        a.`id`,
        a.`name`,
        a.`code`,
        a.`level`,
        a.`lcd`,
        a.`lcu`
        from `autoexec_script_validate` a
        join `autoexec_script_validate_type` b
        on a.`id` = b.`validate_id`
        where b.`type` = #{value}
    </select>

</mapper>

